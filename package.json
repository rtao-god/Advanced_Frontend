    "i18next-http-backend": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.0.0",
    "react-router-dom": "^6.23.1",
    "zustand": "^4.5.2"
  },
