{
    "name": "vite-project",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "main": "src/app/main.tsx",
    "scripts": {
        "dev": "vite",
        "build": "vite build && open analyse.html",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "lint:eslint": "eslint src/**/*.{ts,tsx,js,jsx} --fix",
        "lint:stylelint": "stylelint src/**/*.{scss,sass} --fix",
        "lint": "npm run lint:eslint && npm run lint:stylelint",
        "prettier": "prettier --write ./**/*.{sass,scss}",
        "//1": "generate-structure",
        "gs": "struct src -o src-structure.tree",
        "prepare": "husky",
        "precommit": "npm run gs"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run precommit && lint-staged && git add ."
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "npm run lint:eslint && npm run prettier"
        ],
        "*.{css,scss,sass}": [
            "npm run lint:stylelint && npm run prettier"
        ]
    },
