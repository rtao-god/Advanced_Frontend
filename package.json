{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && open analyse.html",
    "preview": "vite preview",
    "test": "jest",
    "eject": "react-scripts eject",
    "lint:eslint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:stylelint": "stylelint 'src/**/*.{scss,sass}' --fix",
    "lint": "npm run lint:stylelint",
    "//1": "generate-structure",
    "gs": "struct src -o src-structure.tree",
    "prepare": "husky",
    "precommit": "npm run gs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,sass}": [
      "stylelint --fix"
    ],
    "src/**/*": [
      "npm run gs"
    ]
  },
  "overrides": {
    "eslint": "^9.3.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0"
  },
